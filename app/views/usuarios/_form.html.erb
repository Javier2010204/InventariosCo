<%= form_with(model: @user, local: true) do |form| %>
    <% if @user.errors.any? %>
        <div id="error_explanation">
        <h2><%= pluralize(@user.errors.count, "error") %> prohibited this list from being saved:</h2>

        <ul>
        <% @user.errors.full_messages.each do |message| %>
            <li><%= message %></li>
        <% end %>
        </ul>
        </div>
    <% end %>
    Email: <%= form.email_field :email %>
    Password: <%= form.password_field :password %>
    Rol: <%= form.number_field :rol_id %>
    <%= form.submit "Enviar" %>
<% end %>